      "axis": {"title": null, "grid": false, "labelSeparation": 10}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "",
      "scale": {"domain": [2000, 12000]},
      "axis": {"grid": true, "format": "$.2s"}
    }
  },
  "config": {}
}